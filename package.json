{
	"name": "ruby-go-to",
	"displayName": "Ruby Go-to",
	"description": "A VS Code extension for Ruby",
	"version": "0.0.1",
	"publisher": "yuqi",
	"engines": {
		"vscode": "^1.36.0"
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/yyyy7/vscode-rubysonari"
	},
	"keywords": [
        "Ruby",
        "autocomplete",
        "intellisense",
        "YARD",
        "language server"
    ],
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:ruby",
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.36",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"vscode-languageclient": "^5.2.1"
	}
}
